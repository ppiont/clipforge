{
  "extends": "stylelint-config-standard",
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "import",
          "tailwind",
          "custom-variant",
          "theme",
          "apply",
          "layer",
          "config"
        ]
      }
    ]
  }
}